	Journal-Full = {American journal of human genetics},
	Mesh = {Arthritis, Rheumatoid; Breast Neoplasms; Gene Expression Regulation; Genetic Variation; Genome, Human; Histones; Humans; Molecular Sequence Annotation; Quantitative Trait Loci},
	Month = {Jul},
	Number = {1},
