	Mesh = {Algorithms; Computer Simulation; Data Interpretation, Statistical; Expressed Sequence Tags; Gene Expression Profiling; Likelihood Functions; Models, Genetic; Models, Statistical; Poisson Distribution; Sequence Analysis, DNA; Signal Processing, Computer-Assisted},
	Month = {Nov},
	Number = {21},
	Pages = {2881-7},
	Pmid = {17881408},
