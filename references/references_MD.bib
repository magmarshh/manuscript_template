	Doi = {10.1093/nar/gkr1125},
	Journal = {Nucleic Acids Res},
	Journal-Full = {Nucleic acids research},
	Mesh = {Computer Graphics; Databases, Protein; Enzymes; Histones; Humans; Knowledge Bases; Protein Processing, Post-Translational; User-Computer Interface},
