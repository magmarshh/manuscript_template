	Mesh = {Animals; Cell Line; Chromatin; Chromatin Assembly and Disassembly; Chromosome Mapping; Computer Simulation; Drosophila melanogaster; Genome, Insect; Models, Molecular; Nucleosomes; Polytene Chromosomes; Sequence Analysis, RNA; Transcription, Genetic},
	Month = {Jan},
