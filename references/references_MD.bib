	Doi = {10.1038/ng.3379},
	Journal = {Nat Genet},
	Journal-Full = {Nature genetics},
	Mesh = {Autoimmune Diseases; Case-Control Studies; Epistasis, Genetic; Genetic Association Studies; Genetic Loci; Genetic Predisposition to Disease; HLA-C Antigens; HLA-D Antigens; Haplotypes; Histocompatibility Antigens Class I; Humans; Polymorphism, Single Nucleotide; Risk Factors},
