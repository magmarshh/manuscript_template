	Date-Modified = {2019-02-05 15:34:31 -0500},
	Doi = {10.1093/bioinformatics/btl301},
	Journal = {Bioinformatics},
	Journal-Full = {Bioinformatics (Oxford, England)},
	Mesh = {Computer Graphics; Computer Simulation; Models, Biological; Proteome; Signal Transduction; User-Computer Interface},
	Month = {Jan},
	Number = {2},
	Pages = {e177-83},
	Pmid = {17237089},
	Pst = {ppublish},
	Title = {Biological network comparison using graphlet degree distribution},
	Volume = {23},
