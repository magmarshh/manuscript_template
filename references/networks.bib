	Doi = {10.1371/journal.pgen.1000534},
	Journal = {PLoS Genet},
	Journal-Full = {PLoS genetics},
	Mesh = {Crohn Disease; Databases, Genetic; Gene Deletion; Genome, Human; Genome-Wide Association Study; Genomics; Humans; Meta-Analysis as Topic; Polymorphism, Single Nucleotide; Schizophrenia},
	Month = {Jun},
	Number = {6},
