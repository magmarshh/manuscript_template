	Mesh = {Animals; Cell Nucleus; Cells, Cultured; Chromatin; Genomics; Humans; Mice; Mice, Inbred C57BL},
	Month = {Aug},
	Pages = {175},
	Pmc = {PMC4580221},
	Pmid = {26306623},
	Pst = {epublish},
	Title = {Comparison of Hi-C results using in-solution versus in-nucleus ligation},
	Volume = {16},
	Year = {2015},
