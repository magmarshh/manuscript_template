	Eprint = {https://www.biorxiv.org/content/early/2017/08/28/147850.full.pdf},
	Journal = {bioRxiv},
	Publisher = {Cold Spring Harbor Laboratory},
	Title = {HiCcompare: a method for joint normalization of Hi-C datasets and differential chromatin interaction detection},
	Url = {https://www.biorxiv.org/content/early/2017/08/28/147850},
	Year = {2017},
	Bdsk-Url-1 = {https://www.biorxiv.org/content/early/2017/08/28/147850},
	Bdsk-Url-2 = {http://dx.doi.org/10.1101/147850}}

