	Keywords = {CD; CNV; Crohn's disease; MNV; RNA sequencing; RNA-seq; SNP; SNV; SV; copy number variation; gene expression; gene regulation; genetic variation; multinucleotide variant; single-nucleotide polymorphism; single-nucleotide variant; structural variation; transcription},
	Mesh = {Gene Expression Regulation; Genetic Variation; Genome, Human; Humans; Sequence Analysis, DNA; Sequence Analysis, RNA},
	Month = {Nov},
