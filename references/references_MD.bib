	Mesh = {Algorithms; Cell Line, Tumor; Cluster Analysis; Databases, Nucleic Acid; Drug Screening Assays, Antitumor; Gene Expression Profiling; Gene Expression Regulation, Neoplastic; Humans; National Institutes of Health (U.S.); Neoplasms; Signal Transduction; United States},
	Month = {Mar},
