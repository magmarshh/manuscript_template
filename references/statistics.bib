	Journal = {Bioinformatics},
	Journal-Full = {Bioinformatics (Oxford, England)},
	Mesh = {Alzheimer Disease; Case-Control Studies; Computational Biology; Data Interpretation, Statistical; Diabetes Mellitus, Type 2; Gene Expression Profiling; Gene Regulatory Networks; Genome, Human; Genomics; Humans; Leukemia, Myeloid, Acute; Meta-Analysis as Topic; Signal Transduction},
