	Doi = {10.1093/hmg/ddu733},
	Journal = {Hum Mol Genet},
	Journal-Full = {Human molecular genetics},
	Mesh = {Computational Biology; Exome; Genome, Human; Humans; Polymorphism, Single Nucleotide; Software},
