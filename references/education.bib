	Doi = {10.1093/bioinformatics/bts635},
	Journal = {Bioinformatics},
	Journal-Full = {Bioinformatics (Oxford, England)},
	Mesh = {Algorithms; Cluster Analysis; Gene Expression Profiling; Genome, Human; Humans; RNA Splicing; Sequence Alignment; Sequence Analysis, RNA; Software},
	Month = {Jan},
	Number = {1},
