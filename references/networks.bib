	Mesh = {Breast Neoplasms; Female; Gene Regulatory Networks; Genome-Wide Association Study; Humans; Pancreatic Neoplasms; Polymorphism, Single Nucleotide; Protein Interaction Mapping; Software},
	Month = {Jan},
	Number = {1},
