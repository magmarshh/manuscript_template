	Journal = {Wiley Interdiscip Rev Dev Biol},
	Journal-Full = {Wiley interdisciplinary reviews. Developmental biology},
	Mesh = {Animals; Chromatin; Chromatin Assembly and Disassembly; Gene Expression Regulation, Developmental; Genes, Developmental; Genetic Loci; Humans},
