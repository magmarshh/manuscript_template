	Mesh = {Computational Biology; Databases, Genetic; Diagnosis, Differential; Genetic Diseases, Inborn; Genome, Human; Genomics; Humans; Internet; Models, Genetic; Models, Statistical; Monte Carlo Method; Pattern Recognition, Automated; Phenotype; Software; Vocabulary, Controlled},
