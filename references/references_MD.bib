	Mesh = {DNA Methylation; Epigenomics; Female; Humans; Molecular Sequence Data; Prognosis; Triple Negative Breast Neoplasms},
	Month = {Feb},
	Pages = {5899},
	Pmid = {25641231},
	Pst = {epublish},
	Title = {Methylome sequencing in triple-negative breast cancer reveals distinct methylation clusters with prognostic value},
	Volume = {6},
	Year = {2015},
