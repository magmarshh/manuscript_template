	Mesh = {Animals; Binding Sites; CCCTC-Binding Factor; Cell Cycle Proteins; Cells, Cultured; Chromosomal Proteins, Non-Histone; Enhancer Elements, Genetic; Gene Expression Regulation; Histones; Mice; Multigene Family; Promoter Regions, Genetic; Protein Binding; Repressor Proteins; Thymocytes},
	Month = {Apr},
	Number = {4},
	Pages = {504-13},
	Pmc = {PMC4381522},
	Pmid = {25677180},
	Pst = {ppublish},
	Title = {Spatial enhancer clustering and regulation of enhancer-proximal genes by cohesin},
	Volume = {25},
	Year = {2015},
