	Mesh = {Carcinoma, Pancreatic Ductal; Cell Line, Tumor; Gene Expression Profiling; Gene Library; Humans; Internet; Linear Models; Pancreatic Neoplasms; RNA, Messenger; ROC Curve; User-Computer Interface},
	Month = {Jun},
	Pages = {165},
	Pmc = {PMC1189357},
	Pmid = {15987513},
	Pst = {epublish},
	Title = {Identifying differential expression in multiple SAGE libraries: an overdispersed log-linear model approach},
	Volume = {6},
	Year = {2005},
