	Pmc = {PMC5904163},
	Pmid = {29666371},
	Pst = {epublish},
	Title = {Integration of human adipocyte chromosomal interactions with adipose gene expression prioritizes obesity-related genes from GWAS},
	Volume = {9},
	Year = {2018},
