	Journal = {Epigenetics},
	Journal-Full = {Epigenetics},
	Keywords = {ENCODE; GenomeRunner; autoimmunity; epigenetics; genome},
	Mesh = {Arthritis, Rheumatoid; Binding Sites; Enhancer Elements, Genetic; Epigenesis, Genetic; Genetic Predisposition to Disease; Genome-Wide Association Study; Histones; Humans; Lupus Erythematosus, Systemic; MicroRNAs; Promoter Regions, Genetic; Scleroderma, Systemic; Transcription Factors},
