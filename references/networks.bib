@article{Jeffries:2011aa,
	Abstract = {Systemic lupus erythematosus is a chronic-relapsing autoimmune disease of incompletely understood etiology. Recent evidence strongly supports an epigenetic contribution to the pathogenesis of lupus. To understand the extent and nature of dysregulated DNA methylation in lupus T cells, we performed a genome-wide DNA methylation study in CD4 (+) T cells in lupus patients compared to normal healthy controls. Cytosine methylation was quantified in 27,578 CG sites located within the promoter regions of 14,495 genes. We identified 236 hypomethylated and 105 hypermethylated CG sites in lupus CD4 (+) T cells compared to normal controls, consistent with widespread DNA methylation changes in lupus T cells. Of interest, hypomethylated genes in lupus T cells include CD9, which is known to provide potent T-cell co-stimulation signals. Other genes with known involvement in autoimmunity such as MMP9 and PDGFRA were also hypomethylated. The BST2 gene, an interferon-inducible membrane-bound protein that helps restrict the release of retroviral particles was also hypomethylated in lupus patients. Genes involved in folate biosynthesis, which plays a role in DNA methylation, were overrepresented among hypermethylated genes. In addition, the transcription factor RUNX3 was hypermethylated in patients, suggesting an impact on T-cell maturation. Protein-protein interaction maps identified a transcription factor, HNF4a, as a regulatory hub affecting a number of differentially methylated genes. Apoptosis was also an overrepresented ontology in these interaction maps. Further, our data suggest that the methylation status of RAB22A, STX1B2, LGALS3BP, DNASE1L1 and PREX1 correlates with disease activity in lupus patients.},
	Author = {Jeffries, Matlock A and Dozmorov, Mikhail and Tang, Yuhong and Merrill, Joan T and Wren, Jonathan D and Sawalha, Amr H},
	Date-Added = {2017-01-10 16:22:42 +0000},
	Date-Modified = {2017-01-10 16:22:42 +0000},
	Journal = {Epigenetics},
	Journal-Full = {Epigenetics},
	Mesh = {Adult; Aged; Antigens, CD; Antigens, CD9; Apoptosis; CD4-Positive T-Lymphocytes; Core Binding Factor Alpha 3 Subunit; Cytosine; DNA Methylation; Epigenomics; Female; GPI-Linked Proteins; Genome-Wide Association Study; Humans; Lupus Erythematosus, Systemic; Matrix Metalloproteinase 9; Membrane Glycoproteins; Middle Aged; Promoter Regions, Genetic; Receptor, Platelet-Derived Growth Factor alpha},
