	Mesh = {Algorithms; Bayes Theorem; Carcinoma, Squamous Cell; Gene Expression Profiling; Genetic Variation; High-Throughput Nucleotide Sequencing; Linear Models; Mouth Neoplasms; Sequence Analysis, RNA},
	Month = {May},
	Number = {10},
