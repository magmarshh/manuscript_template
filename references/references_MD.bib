	Journal-Full = {Nucleic acids research},
	Mesh = {Animals; Base Sequence; Chromatin; Conserved Sequence; Databases, Nucleic Acid; Enhancer Elements, Genetic; Genetic Linkage; Genome-Wide Association Study; Genomics; INDEL Mutation; Linkage Disequilibrium; Molecular Sequence Annotation; Nucleotide Motifs; Polymorphism, Single Nucleotide; Regulatory Sequences, Nucleic Acid},
	Month = {Jan},
	Number = {Database issue},
	Pages = {D930-4},
