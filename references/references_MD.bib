	Doi = {10.1093/dnares/dsu044},
	Journal = {DNA Res},
	Journal-Full = {DNA research : an international journal for rapid publication of reports on genes and genomes},
	Keywords = {bioinformatic tool; epigenetics; gene expression; genome tracks; transcription start sites},
	Mesh = {Animals; Caenorhabditis elegans; CpG Islands; Databases, Genetic; Drosophila melanogaster; Gene Expression Regulation; Genome-Wide Association Study; Humans; Mice; Software; Transcription Initiation, Genetic; Zebrafish},
	Month = {Feb},
	Number = {1},
	Pages = {109-19},
