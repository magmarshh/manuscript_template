	Doi = {10.4161/epi.6.12.18176},
	Journal = {Epigenetics},
	Journal-Full = {Epigenetics},
	Mesh = {Databases, Genetic; Epigenesis, Genetic; Gene Regulatory Networks; Histones; Humans; Internet; Software},
	Month = {Dec},
	Number = {12},
