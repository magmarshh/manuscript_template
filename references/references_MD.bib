	Keywords = {Bayesian network; Chromatin boundary; insulator element; random forest},
	Mesh = {Algorithms; Animals; Artificial Intelligence; Base Sequence; Bayes Theorem; Chromatin; Drosophila melanogaster; Insulator Elements; Molecular Sequence Data; Pattern Recognition, Automated; Sequence Analysis, DNA},
	Month = {Dec},
	Number = {6},
	Pages = {1442006},
