	Doi = {10.1371/journal.pmed.1002174},
	Journal = {PLoS Med},
	Journal-Full = {PLoS medicine},
	Mesh = {Aged; Breast Neoplasms; Disease Progression; Female; Genomics; Humans; Middle Aged; Mutation; Neoplasms, Basal Cell; Retrospective Studies},
	Month = {Dec},
	Number = {12},
	Pages = {e1002174},
	Pmc = {PMC5140046},
	Pmid = {27923045},
	Pst = {epublish},
	Title = {Tumor Evolution in Two Patients with Basal-like Breast Cancer: A Retrospective Genomics Study of Multiple Metastases},
	Volume = {13},
