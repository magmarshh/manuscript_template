	Mesh = {Animals; Forelimb; Gene Expression Regulation, Developmental; Gene Order; Genes, Homeobox; Genes, Switch; Mice; Mice, Inbred C57BL; Mice, Transgenic; Multigene Family; Telomere; Transcription, Genetic},
	Month = {Jun},
	Number = {6137},
