	Mesh = {Cell Transformation, Neoplastic; Genes, Neoplasm; Genetic Heterogeneity; Genome, Human; Humans; Mutagenesis; Mutation; Neoplasms; Signal Transduction},
	Month = {Mar},
	Number = {6127},
	Pages = {1546-58},
	Pmc = {PMC3749880},
	Pmid = {23539594},
