	Journal = {BMC Genomics},
	Journal-Full = {BMC genomics},
	Keywords = {3C; 4C; 5C; 6C; 7C; ChIA-PET; ChIP-seq; Chromatin interactions; Chromatin loops; Hi-C; Prediction; Three-dimensional genome architecture; Transcription factors},
	Month = {Oct},
	Number = {1},
	Pages = {777},
	Pmc = {PMC6814980},
	Pmid = {31653198},
	Pst = {epublish},
	Title = {7C: Computational Chromosome Conformation Capture by Correlation of ChIP-seq at CTCF motifs},
