	Journal = {Bioinformatics},
	Journal-Full = {Bioinformatics (Oxford, England)},
	Mesh = {Algorithms; Breast Neoplasms; Cluster Analysis; Computational Biology; DNA Copy Number Variations; Databases, Genetic; Female; Gene Expression Profiling; Humans; Lung Neoplasms; Models, Statistical},
	Month = {Nov},
	Number = {22},
	Pages = {2906-12},
	Pmc = {PMC2800366},
	Pmid = {19759197},
	Pst = {ppublish},
	Title = {Integrative clustering of multiple genomic data types using a joint latent variable model with application to breast and lung cancer subtype analysis},
	Volume = {25},
	Year = {2009},
