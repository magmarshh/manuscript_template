	Keywords = {Hi-C; clustering algorithms; metagenome assembly; metagenomics},
	Mesh = {Algorithms; Archaea; Bacteria; Cluster Analysis; Contig Mapping; Fungi; Genome, Archaeal; Genome, Bacterial; Genome, Fungal; Metagenomics; Sequence Analysis, DNA},
