	Doi = {10.1073/pnas.0400054101},
	Journal = {Proc Natl Acad Sci U S A},
	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Mesh = {Algorithms; Football; Humans; Metabolism; Models, Biological; Neural Networks (Computer); Phylogeny; Proteins; Reproducibility of Results},
	Month = {Mar},
	Number = {9},
	Pages = {2658-63},
